{
  "version": "0.2.0",
  "configurations": [{
    "name": "Debug (ST-LINK + OpenOCD)",
    "type": "cortex-debug",
    "request": "launch",
    "servertype": "openocd",
    "liveWatch": true,
    "cwd": "${workspaceFolder}",
    "executable": "${workspaceFolder}/build/CNCv1",
    "svdFile": "${workspaceFolder}/svd/STM32F446.svd",
    "device": "STM32F446RE",
    "configFiles": ["interface/stlink.cfg","target/stm32f4x.cfg"],
    "openOCDLaunchCommands": ["transport select swd","adapter speed 950"],
    "runToEntryPoint": "main", //prev: "runToMain": true,
    "preLaunchTask": "Build (CMake/Ninja)"
  }]
}
